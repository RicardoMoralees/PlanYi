    <string name="title_musica">MÚSICA</string>
    <string name="musica1">GRUPO VERSÁTIL</string>
    <string name="musica2">Café café BANDA ORQUESTA</string>
    <string name="musica1_desc">Una exelente opcción para aquellos que buscan "un poco de todo" con ellos, seguro que cada uno de tus invitados pasara un momento inolvidable.</string>
    <string name="musica2_desc">Una de las bandas mas cotizadas del estado de Sinaloa, contamos con más de 30 años de tradición y una experiencia que seguro nunca olvidarás.</string>
    <string name="musica1_costo">Costo: $60,000</string>
    <string name="musica2_costo">Costo: $65,000</string>

